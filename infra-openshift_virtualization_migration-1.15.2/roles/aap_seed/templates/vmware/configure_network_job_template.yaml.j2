name: Configure Networks - {{ _target_name }} - {{ _mf_host}}
organization: "{{ aap_seed_aap_org_name }}"
project: "{{ aap_seed_aap_project }}"
job_type: run
ask_variables_on_launch: true
extra_vars: 
  {{ aap_seed_aap_job_template_extra_vars | ansible.builtin.combine(migration_target['network'] | default({})) | to_yaml | indent(2) }}
playbook: playbooks/network_mgmt.yml
inventory: "{{ aap_seed_aap_inventory }}"
execution_environment: "{{ aap_seed_aap_execution_environment }}"
credentials:
  - {{ _kubeconfig_credential }}
  - {{ _target_credential }}
  - {{ _mf_cac_credential }}
